{
  "name": "tap-example",
  "description": "Example tap for Singer SDK",
  "version": "0.1.1",
  "sdk_version": "1.0.0",
  "capabilities": [
    "catalog",
    "discover",
    "state"
  ],
  "settings": {
    "properties": {
      "start_date": {
        "type": "string",
        "format": "date-time",
        "description": "Start date for the tap to extract data from."
      },
      "api_key": {
        "type": "string",
        "description": "API key for the tap to use."
      }
    },
    "required": [
      "api_key"
    ]
  }
}